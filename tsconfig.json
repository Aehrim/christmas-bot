{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "rootDir": "./src",
      "outDir": "./dist",
      "removeComments": true,
      "resolveJsonModule": true,
      // "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": false,
      "strictNullChecks": true,
      "skipLibCheck": true
    }
  }
  